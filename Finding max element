#include <bits/stdc++.h>
using namespace std;

int main()
{
    float ar[10]={1,2,3,4,5,6,7,8,10,9};
    float max_element=ar[0];
    
    for (int i=0;i<10;i++)
    {
        if (max_element<ar[i])
        {
            max_element=ar[i];
        }
    }
    cout<<max_element;
    
    return 0;
}
